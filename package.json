{
  "name": "discord-bot-template",
  "author": "Christopher Scott",
  "license": "MIT",
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "env NODE_ENV=DEVELOPMENT nodemon --es-module-specifier-resolution=node --no-warnings -r dotenv/config app.js",
    "debug": "env NODE_ENV=DEBUG nodemon --es-module-specifier-resolution=node --no-warnings -r dotenv/config app.js",
    "start": "env NODE_ENV=PRODUCTION node --es-module-specifier-resolution=node --no-warnings app.js",
    "kill": "pkill -9 node"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@discordjs/voice": "^0.8.0",
    "bufferutil": "^4.0.3",
    "colors": "^1.4.0",
    "discord-api-types": "^0.29.0",
    "discord.js": "^13.1.0",
    "erlpack": "discord/erlpack",
    "nanoexpress": "^5.2.2",
    "nanoid": "^3.1.25",
    "node-schedule": "^2.0.0",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/node-schedule": "^1.3.2",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.12"
  }
}
